        "@storybook/blocks": "^7.5.0",
        "@storybook/react": "^7.5.0",
        "@storybook/react-webpack5": "^7.5.0",
        "@storybook/testing-library": "^0.2.2",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/react": "^14.0.0",
        "@types/json-server": "^0.14.6",
